local function KDZESRA_script() -- Option_12.function 
	local script = Instance.new('LocalScript', Option_12)

	local mats = script.Parent.Parent.Parent[" Materializer"].Frame
	
	script.Parent.MouseButton1Click:connect(function() 
		if mats.Visible == false then 
			mats.Visible = true 
		else
			mats.Visible = false 
		end
	end)
	
end
coroutine.wrap(KDZESRA_script)()
