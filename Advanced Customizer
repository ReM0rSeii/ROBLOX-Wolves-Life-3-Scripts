-- Scripts:

local function SXUY_script() -- Option.Script 
	local script = Instance.new('Script', Option)

	function Click() 
	
		local args = {
			[1] = "Fluff",
			[2] = "InsideEars",
			[3] = 0
		}
	
		game:GetService("ReplicatedStorage").MasterKey:FireServer(unpack(args))
		local args = {
			[1] = "Fluff",
			[2] = "LeftEar",
			[3] = 0
		}
	
		game:GetService("ReplicatedStorage").MasterKey:FireServer(unpack(args))
		local args = {
			[1] = "Fluff",
			[2] = "RightEar",
			[3] = 0
		}
	
		game:GetService("ReplicatedStorage").MasterKey:FireServer(unpack(args))
	
	end 
	
	script.Parent.MouseButton1Down:connect(Click) 
end
coroutine.wrap(SXUY_script)()
local function ACMSWX_script() -- Option_2.Script 
	local script = Instance.new('Script', Option_2)

	function Click() 
	
		local args = {
			[1] = "Fluff",
			[2] = "DragonPrimary",
			[3] = 1
		}
	
		game:GetService("ReplicatedStorage").MasterKey:FireServer(unpack(args))
	
	end 
	
	script.Parent.MouseButton1Down:connect(Click) 
end
